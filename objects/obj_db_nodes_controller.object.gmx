    draw_text(16,room_height-16,"Left click anywhere to add nodes. Right click a node to delete it.");    
}
else {
    draw_text(16,room_height-16,"Nodes connected by: "+orders[| current_order]+ " (use &lt; and &gt; arrow keys to change)");
    
    draw_set_color(c_dkgray);
    draw_set_valign(fa_top);
    
