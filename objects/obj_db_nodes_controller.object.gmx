        var node = nodes[| i];
        draw_text(16,16+i*24,
            string(node[? "id"])+ ": " +
            node[? "label"]+ ", " +
            string(node[? "x"])+ ", " +
            string(node[? "y"]));
        if(i &gt; 0) {
            var prev_node = nodes[| i-1];
            draw_arrow(prev_node[? "x"],prev_node[? "y"],node[? "x"],node[? "y"],8);
        }
    }
}
